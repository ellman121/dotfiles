alias l='ls -CF'
alias la='ls -A'
alias ll='ls -alhF'
alias lm='lsmac -alF | grep /'
alias mkdir='mkdir -p'
alias SimpleHTTP='python ~/bin/SimpleHTTP.py'
alias SimpleFTP='~/bin/SimpleFTP.sh'
alias ytdla='youtube-dl --extract-audio --audio-quality 0 --audio-format mp3'
alias ..='cd ..'
alias ...='cd ../..'
alias webget='wget -rkpN --reject "index.html*" -c --no-parent -e robots=off '
alias proxy='~/bin/enableSOCKS && ssh -NTD 9050 && ~/bin/disableSOCKS'
alias pubip='dig TXT +short o-o.myaddr.l.google.com @ns1.google.com'
alias nom='rm -rf node_modules && mkdir node_modules && touch node_modules/.metadata_never_index && npm install'
